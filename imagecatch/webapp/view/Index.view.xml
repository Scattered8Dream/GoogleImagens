<mvc:View controllerName="imagecatch.controller.Index" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <FlexBox justifyContent="Center">
                <Input id="ip_busca" />
                <Button text="Buscar" press="onPressBuscar" />
            </FlexBox>

            <List headerText="Lista de Resultados">
                <CustomListItem>
                    <HBox>
                        <Image src="https://cdn.albumoftheyear.org/album/78827-coisas.jpg" width="100px" heigth="100px" />
                        <VBox>
                            <Link text="Moacir Santos - Coisas" href="https://cdn.albumoftheyear.org/album/78827-coisas.jpg" target="_blank" />
                            <Label text="Moacir Santos - Coisas - Reviews - Album of The Year" />
                        </VBox>
                    </HBox>
                </CustomListItem>
            </List>
        </content>
    </Page>
</mvc:View>
